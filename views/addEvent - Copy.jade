										.col-sm-2
											input.form-control(type='text', id='', placeholder='No. of Table', name="viptablecount", value='#{event.viptable[0].tablecount}')
										.col-sm-2
											input.form-control(type='text', id='', placeholder='Table Price', name="viptableprice", value='#{event.viptable[0].tableprice}')
									.bg-default.text-center
										button.btn.btn-lg.btn-primary(type='submit') Save

							- else
