													i.icon-calendar
												input.form-control.bootstrap-datepicker(type='text', id='', name='fromdate', data-format='dd/MM/yyyy HH:mm:ss PP', placeholder='From Date' required)
										.col-sm-3
											.datetimepicker.input-prepend.input-group
												span.add-on.input-group-addon
													i.icon-calendar
