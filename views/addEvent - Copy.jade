												option Hip-Hop
												option Rock
												option Jazz
												option Pop
												option Reggae
												option R&B
												option Blues
												option Country
									.form-group
										label.col-sm-3.control-label DJ:
										.col-sm-6
											select.chosen-select(name='dj', multiple, data-placeholder='Select multiple DJ...' required)
												- each dj, i in alldj
													option(value='#{dj._id}') #{dj.djname}
									.bg-default.text-center
										a#nxtmedia.btn.btn-lg.btn-primary(href='#media', data-toggle='tab') Next
								#media.tab-pane.fade
									.form-group
										label.col-sm-3.control-label Cover Image:
										.col-sm-6
											input.form-control(type='file', name="coverimage" required)
									.form-group
