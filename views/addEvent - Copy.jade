									.form-group
										label.col-sm-3.control-label Entry Fee:
										.col-sm-6
											input.form-control(type='text', id='', placeholder='Entry Fee...', name="entryfee" required)
									.form-group
										label.col-sm-3.control-label Details:
										.col-sm-6
											textarea.form-control.textarea-autosize(name='eventdetails', rows='3' required)
									.form-group
										label.col-sm-3.control-label Genres
										.col-sm-6
