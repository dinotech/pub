			ul#myTab.nav.clearfix.nav-tabs
				li.basic.active
					a(href='#basic', data-toggle='tab') Basic
				li.medias
					a(href='#media', data-toggle='tab') Media
				li.advanced
					a(href='#advanced', data-toggle='tab') Advanced
				li.promotion
					a(id='promotion', data-toggle='tab') Promotion
			.panel
				.panel-body
					.example-box-wrapper.phase1
						#myTabContent.tab-content
						- if (flash && flash.successmsg && flash.successmsg.length > 0)
							p.alert.alert-success= flash.successmsg
						- if (flash && flash.errormsg && flash.errormsg.length > 0)
							p.alert.alert-danger= flash.errormsg
						form.form-horizontal.bordered-row(action='', method='post', enctype='multipart/form-data')
							input.form-control(type='hidden', id='', name="userid", value='#{flash.id}')
							- if (event)
								input.form-control(type='hidden', id='', name="oldcoverimage", value='#{event.coverimage}')
								input.form-control(type='hidden', id='', name="oldeventimage", value='#{event.eventimage}')
								#basic.tab-pane.fade.active.in
									.form-group
										label.col-sm-3.control-label Title:
										.col-sm-6
											input.form-control(type='text', id='', placeholder='Event Name...', name="eventname", value='#{event.eventname}')
									.form-group
										label.col-sm-3.control-label Date & Time:
										.col-sm-3
											.datetimepicker.input-prepend.input-group
												span.add-on.input-group-addon
													i.icon-calendar
												input.form-control.bootstrap-datepicker(type='text', id='', name='fromdate', data-format='dd/mm/yyyy HH:mm:ss PP', placeholder='From Date', value='#{event.fromdate}')
										.col-sm-3
											.datetimepicker.input-prepend.input-group
												span.add-on.input-group-addon
													i.icon-calendar
												input.form-control.bootstrap-datepicker(type='text', id='', name='todate', data-format='dd/mm/yyyy HH:mm:ss PP', placeholder='To Date', value='#{event.todate}')
									.form-group
										label.col-sm-3.control-label Entry Fee:
										.col-sm-6
											input.form-control(type='text', id='', placeholder='Entry Fee...', name="entryfee", value='#{event.entryfee}')
									.form-group
										label.col-sm-3.control-label Details:
										.col-sm-6
											textarea.form-control.textarea-autosize(name='eventdetails', rows='3' ) #{event.eventdetails}
									.form-group
										label.col-sm-3.control-label Genres
										.col-sm-6
											select.chosen-select(name='genres', multiple, data-placeholder='Select multiple Genres...')
												option Hip-Hop
												option Rock
												option Jazz
												option Pop
												option Reggae
												option R&B
												option Blues
												option Country
									.form-group
										label.col-sm-3.control-label DJ:
										.col-sm-6
											select.chosen-select(name='dj', multiple, data-placeholder='Select multiple DJ...')
												- each dj, i in alldj
													option(value='#{dj._id}') #{dj.djname}
									.bg-default.text-center
										a#nxtmedia.btn.btn-lg.btn-primary(href='#media', data-toggle='tab') Next
								#media.tab-pane.fade
									.form-group
										label.col-sm-3.control-label Cover Image:
										.col-sm-6
											input.form-control(type='file', name="coverimage" )
									.form-group
										label.col-sm-3.control-label Slider Images:
										.col-sm-6
											input.form-control(type='file', name="eventimage", multiple )
										.col-sm-3 (you can insert multiple images.)
									.bg-default.text-center
										a#nxtadvanced.btn.btn-lg.btn-primary(href='#advanced', data-toggle='tab') Next
								#advanced.tab-pane.fade
									.form-group
										label.col-sm-3.control-label STANDARD TABLE:
										.col-sm-2
											input.form-control(type='text', id='', placeholder='Up to Guest', name="stdgusetlimit", value='#{event.standardtable[0].guestlimit}')
										.col-sm-2
											input.form-control(type='text', id='', placeholder='No. of Table', name="stdtablecount", value='#{event.standardtable[0].tablecount}')
										.col-sm-2
											input.form-control(type='text', id='', placeholder='Table Price', name="stdtableprice", value='#{event.standardtable[0].tableprice}')
									.form-group
										label.col-sm-3.control-label PREMIUM TABLE:
										.col-sm-2
											input.form-control(type='text', id='', placeholder='Up to Guest', name="pregusetlimit", value='#{event.premiumtable[0].guestlimit}')
										.col-sm-2
											input.form-control(type='text', id='', placeholder='No. of Table', name="pretablecount", value='#{event.premiumtable[0].tablecount}')
										.col-sm-2
											input.form-control(type='text', id='', placeholder='Table Price', name="pretableprice", value='#{event.premiumtable[0].tableprice}')
